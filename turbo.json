{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build:shared"
      ],
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:fix:sfui": {
      "cache": false,
      "outputs": []
    },
    "build:next": {
      "cache": false,
      "dependsOn": [
        "^build:react",
        "^build:typography"
      ]
    },
    "build:nuxt": {
      "cache": false,
      "dependsOn": [
        "^build:vue",
        "^build:typography"
      ]
    },
    "build:react": {
      "dependsOn": [
        "^build:shared"
      ],
      "cache": false
    },
    "build:vue": {
      "dependsOn": [
        "^build:shared"
      ],
      "cache": false
    },
    "build:release": {
      "dependsOn": [
        "build:react", "build:vue", "build:css"
      ],
      "cache": false
    },
    "build:shared": {
      "cache": false
    },
    "build:typography": {
      "cache": false
    },
    "build:test-utils": {
      "cache": false
    },
    "build:css": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:docs": {
      "cache": false
    },
    "dev:shared": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build:test-utils",
        "dev:shared"
      ],
      "cache": false
    },
    "test:ci:vue": {
      "dependsOn": [
        "^build:shared",
        "build:test-utils"
      ],
      "cache": false
    },
    "test:ci:react": {
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:test-utils"
      ],
      "cache": false
    }
  }
}
